<table>
    <thead>
        <tr>
            <td>Name</td>
        </tr>
    </thead>
    <% cats.forEach(cat => {%>
        <tr>
            <td>
                <img width="200" height="auto" src="<%= cat.imgUrl %>" />
                <%= cat.name %>
                    <%= cat.location %>
            </td>
            <td>
                <a href="/cat/show/<%= cat._id %>" class="btn btn-primary btn-block">View</a>

            </td>
        </tr>
        <% }) %>
</table>